{
  "id": "babele-translation-files-generator",
  "title": "Translation files generator for Babele",
  "description": "Generate translation file for Babele by selecting a valid Compendium.",
  "authors": [
    {
      "name": "DjLeChuck",
      "email": "djlechuck@gmail.com",
      "url": "https://github.com/djlechuck/",
      "flags": {}
    }
  ],
  "version": "#{VERSION}#",
  "compatibility": {
    "minimum": "11",
    "verified": "12.324"
  },
  "esmodules": [
    "assets/btfg.js"
  ],
  "styles": [
    "assets/btfg.css"
  ],
  "languages": [
    {
      "lang": "en",
      "name": "English",
      "path": "lang/en.json"
    },
    {
      "lang": "fr",
      "name": "Fran√ßais",
      "path": "lang/fr.json"
    }
  ],
  "url": "#{URL}#",
  "manifest": "#{MANIFEST}#",
  "download": "#{DOWNLOAD}#",
  "bugs": "https://github.com/DjLeChuck/foundryvtt-babele-translation-files-generator/issues",
  "changelog": "https://github.com/DjLeChuck/foundryvtt-babele-translation-files-generator/blob/main/CHANGELOG.md",
  "license": "./LICENSE",
  "readme": "./README.md",
  "flags": {
    "hotReload": {
      "extensions": [
        "css",
        "hbs",
        "json"
      ],
      "paths": [
        "lang/",
        "assets/",
        "templates/"
      ]
    }
  }
}
